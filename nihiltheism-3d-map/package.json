{
  "name": "nihiltheism-3d-knowledge-map",
  "version": "1.0.0",
  "type": "module",
  "description": "3D Knowledge Graph Application for Nihiltheism Philosophy",
  "main": "backend/server.ts",
  "scripts": {
    "dev": "concurrently \"npm run dev:frontend\" \"npm run dev:backend\" \"npm run dev:ai\"",
    "dev:frontend": "vite",
    "dev:backend": "nodemon --watch backend --ext ts,js --exec ts-node -P tsconfig.server.json backend/server.ts",
    "dev:ai": "cd ai-service && uvicorn main:app --reload --port 8001",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "setup": "node scripts/setup-db.js",
    "seed": "node scripts/seed-data.js",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "format": "prettier --write \"src/**/*.{ts,tsx,css}\" \"backend/**/*.ts\""
  },
  "dependencies": {
    "@react-three/drei": "^9.96.0",
    "@react-three/fiber": "^8.15.0",
    "body-parser": "^1.20.2",
    "compromise": "^14.11.0",
    "cors": "^2.8.5",
    "date-fns": "^3.0.6",
    "dexie": "^3.2.4",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "fuse.js": "^7.0.0",
    "helmet": "^7.1.0",
    "ml-distance": "^4.0.1",
    "mongodb": "^6.3.0",
    "natural": "^6.10.4",
    "openai": "^4.24.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "three": "^0.160.0",
    "uuid": "^9.0.1",
    "zustand": "^4.5.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/express-rate-limit": "^6.0.0",
    "@types/helmet": "^0.0.48",
    "@types/node": "^20.10.6",
    "@types/react": "^18.2.47",
    "@types/react-dom": "^18.2.18",
    "@types/three": "^0.160.0",
    "@types/uuid": "^9.0.7",
    "@vitejs/plugin-react": "^4.2.1",
    "concurrently": "^8.2.2",
    "eslint": "^8.56.0",
    "nodemon": "^3.0.2",
    "prettier": "^3.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3",
    "vite": "^5.0.11"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}
